  "devDependencies": {
    "@marijn/buildtool": "^0.1.5",
    "@types/node": "^20.6.2",
    "typescript": "^5.2.2"
  },
  "files": ["dist"],
  "license": "MIT",
  "dependencies": {
    "@lezer/common": "^1.1.0"
  }
}
