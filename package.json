{
  "name": "printer",
  "version": "1.0.0",
  "description": "Parse tree printer for the lezer incremental parser",
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "module": "dist/index.js",
